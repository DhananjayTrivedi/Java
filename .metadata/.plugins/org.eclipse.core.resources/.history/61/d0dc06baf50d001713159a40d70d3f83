package programs;

import java.util.*;
import java.util.Scanner;

public class RevengeOfPancakes {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		String s = input.next();
		System.out.println(s);
		int index;
		//Starting Pancakes
		for (int i = 0; i < s.length(); i++) {
			char a = s.charAt(i);
				if(a == '-')
					continue;
				else{
					index = i;
					break;
				}
		
		}
		
		s.substring(0,index);


	}

}
