import java.util.*;

public class Sets {

	public static void main(String[] args) {
		Set<String> set = new HashSet<>();
		set.add("Hello");
		set.add("Hello");
		set.add("Hello");
		set.add("Friends");
		set.add("Hello");
		
		System.out.println(set);
		
		System.out.println(set.size());
		set.remove("Hello");
		
		System.out.println(set);
		
		for(String s: set){
			System.out.println(s);
		}
		
	}

}
