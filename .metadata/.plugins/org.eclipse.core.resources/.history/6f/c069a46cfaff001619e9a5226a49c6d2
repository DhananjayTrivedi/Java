
public class StringExampless {

	public static void main(String[] args) {
		String s = "Hello";
		System.out.println(s.charAt(1));
		System.out.println(s.equals("Hello"));
		System.out.println(s.compareTo("Hello"));
		System.out.println(s.compareToIgnoreCase("hello"));
		System.out.println(s.endsWith("o"));
		System.out.println(s.indexOf("hell"));
		System.out.println(s.toLowerCase());
		System.out.println(s.trim());
		System.out.println(s.concat(" hello"));
		System.out.println(s.isEmpty());
		System.out.println(s.lastIndexOf(2, 4));
		System.out.println(s.toUpperCase());
		System.out.println(s.getClass());
		System.out.println(s.split(" "));
	}

}
