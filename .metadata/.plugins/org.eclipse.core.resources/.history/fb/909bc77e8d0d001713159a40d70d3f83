package programs;

import java.util.*;

public class test {

  public static void main(String[] args) {
	  HashMap<Integer,Integer> hm = new HashMap<>();
	  Scanner input = new Scanner(System.in);
	  int a = input.nextInt();
	  String s = String.valueOf(a);
	  for (int i = 0; i < s.length(); i++) {
		System.out.print(s.charAt(i));
		hm.put(s.charAt(i), 1);
	  }
  }
}