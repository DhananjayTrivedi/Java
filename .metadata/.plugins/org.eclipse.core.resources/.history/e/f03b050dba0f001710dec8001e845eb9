package programs;

import java.util.Scanner;

public class Test1 {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int[] arr  = {3,5,7,9,21,25};
		int sum = 0;
		for (int i : arr) {
			checkDigit(i);
			sum += i;
		}
		System.out.println(sum);
	}

	private static boolean checkDigit(int i) {
		int count = 0;
		while(i!=0){
			i = i%10;
			count ++;
		}
		if(count > 1)
			return true;
		else
			return false;
	}

}
