
public class Stack {
	private int[] object;
	private int top;
	private int stackSize;
	
	public Stack(){
		stackSize = 1;
		top = -1;
		object = new int[stackSize];
	}
	public void push(int item){
		if(top>=stackSize - 1){
			resize();
			object[++top] = item; 
		}
	}
	private void resize() {
		int[] temp = object;
		stackSize = stackSize * 2;
		object = new int[stackSize];
	}
	
}
