import java.util.Scanner;

public class StairCase {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int size = input.nextInt();
		for (int i = 0; i < args.length; i++) {
			int d = size - i;
			while(d!=0){
				System.out.print(" ");
			}
			System.out.println();
		}
	}

}
