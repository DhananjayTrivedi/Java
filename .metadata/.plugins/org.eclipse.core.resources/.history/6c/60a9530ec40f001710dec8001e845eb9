import java.util.Scanner;

public class PerfectEquation {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int A = input.nextInt();
		int B = input.nextInt();
		long x = B,y = A;
		System.out.println(x+" "+y);
		
		if(x > y && x % y == 0){
			x = x / y;
		}
		
	}

}
