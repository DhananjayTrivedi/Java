package programs;

import java.util.*;
import java.util.Scanner;

public class RevengeOfPancakes {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		String inputString = input.next();
		System.out.println(inputString);
		checkStart(inputString);
		
		
		}// End of Starting
	}

	private static String checkStart(String s) {
		//CheckStart
		int index = 0;
		String result = null;
		if(s.charAt(0) == '-'){
			for (int i = 0; i < s.length(); i++) {
				char a = s.charAt(i);
					if(a == '-'){
						index = i+1;
						continue;
					}
					else{
						break;
					}
			}
		//Flip and append the String
		result = new StringBuilder(s.substring(0, index+1)).replace(0, index, "+").reverse().toString();
		System.out.println(result);
		System.out.println("Final");
		result += s.substring(index);
		return result;
	}
}