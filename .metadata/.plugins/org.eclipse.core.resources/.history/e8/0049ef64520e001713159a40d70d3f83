package programs;

import java.util.*;
import java.util.Scanner;

public class RevengeOfPancakes {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		String s = input.next();
		System.out.println(s);
		int index = 0;
		//Starting Pancakes
		if(s.charAt(0) == '-'){
			for (int i = 0; i < s.length(); i++) {
				char a = s.charAt(i);
					if(a == '-'){
						index = i;
						continue;
					}
					else{
						break;
					}
			}
			//Flip and append the String
			
			StringBuilder revThis = new StringBuilder(s.subSequence(0, index));  
			
		}
		s.substring(0,index);
	}
	
	 private static String Reverser(String s,int startIndex, int endIndex) {
			String result = s.substring(0, startIndex);
			result += new StringBuilder(s.substring(startIndex, endIndex)).reverse().toString();
			result += s.substring(endIndex);
			return result;

}
